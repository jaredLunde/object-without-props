{
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/env", {"loose": true}],
      ],
      "plugins": [
        "@babel/transform-runtime",
        "closure-elimination"
      ]
    },
    "es": {
      "presets": [
        ["@babel/env", {"loose": true, "modules": false}]
      ],
      "plugins": [
        "closure-elimination"
      ]
    }
  }
}
